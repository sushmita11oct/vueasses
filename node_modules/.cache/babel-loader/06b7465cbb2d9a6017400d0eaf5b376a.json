K,QAAL,CAAc,OAAd,CAAsB,KAAK,KAAL,CAAW,KAAX,EAAkB,MAAxC,CAAf;AACA,UAAI,EAAE,QAAF,GAAa,CAAjB,EAAoB,QAAO,GAAI,CAAX;AACpB,WAAK,KAAL,CAAW,KAAX,EAAkB,MAAlB,GAA2B,KAAK,QAAL,CAAc,QAAd,CAA3B;AACD,KAdM;;AAeP;AACC;AACA;AACD,IAAA,UAlBO,sBAkBI,KAlBJ,EAkBW;AAChB,WAAK,KAAL,CAAW,MAAX,CAAkB,KAAlB,EAAyB,CAAzB;AACD,KApBM;;AAqBP;AACC;AACA;AACD,IAAA,QAxBO,oBAwBE,KAxBF,EAwBS;AACd,WAAK,IAAL,GAAY,KAAK,KAAL,CAAW,KAAX,EAAkB,IAA9B;AACA,WAAK,UAAL,GAAkB,KAAlB;AACD,KA3BM;;AA4BP;AACC;AACA;AACD,IAAA,UA/BO,wBA+BM;AACX,UAAI,KAAK,IAAL,CAAU,MAAV,KAAqB,CAAzB,EAA4B;AAC5B;;AACA,UAAI,KAAK,UAAL,IAAmB,IAAvB,EAA6B;AAC3B,aAAK,KAAL,CAAW,KAAK,UAAhB,EAA4B,IAA5B,GAAmC,KAAK,IAAxC;AACA,aAAK,UAAL,GAAkB,IAAlB;AACF,OAHA,MAGO;AACL;AACA,aAAK,KAAL,CAAW,IAAX,CAAgB;AACd,UAAA,IAAI,EAAE,KAAK,IADG;AAEd,UAAA,MAAM,EAAE;AAFM,SAAhB;AAIF;;AACA,WAAK,IAAL,GAAY,EAAZ;AACD;AA7CM;AA3BI,CAAf","sourcesContent":["<template>\r\n<center>\r\n  <div class=\"container\" style=\"max-width: 600px\">\r\n    <!-- Heading -->\r\n    <h2 class=\"text-center mt-5\">My Vue ToDo List App</h2>\r\n\r\n    <!-- Input -->\r\n    <div class=\"d-flex mt-5\">\r\n      <input\r\n        type=\"text\"\r\n        v-model=\"task\"\r\n        placeholder=\"Enter task\"\r\n        class=\"w-100 form-control\"\r\n      />\r\n      <button class=\"btn btn-warning rounded-0\" @click=\"submitTask\">\r\n        ADD\r\n      </button>\r\n    </div>\r\n\r\n    <!-- Task table -->\r\n    <table class=\"table table-bordered mt-5\" border=\"1\">\r\n      <thead>\r\n        <tr>\r\n          <th scope=\"col\">Task</th>\r\n          <th scope=\"col\" style=\"width: 120px\">Status</th>\r\n          <th scope=\"col\" class=\"text-center\">#</th>\r\n          <th scope=\"col\" class=\"text-center\">#</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(task, index) in tasks\" :key=\"index\">\r\n          <td>\r\n            <span :class=\"{ 'line-through': task.status === 'finished' }\">\r\n              {{ task.name }}\r\n            </span>\r\n          </td>\r\n          <td>\r\n            <span\r\n              class=\"pointer noselect\"\r\n              @click=\"changeStatus(index)\"\r\n              :class=\"{\r\n                'text-danger': task.status === 'to-do',\r\n                'text-success': task.status === 'finished',\r\n                'text-warning': task.status === 'in-progress',\r\n              }\"\r\n            >\r\n              {{ capitalizeFirstChar(task.status) }}\r\n            </span>\r\n          </td>\r\n          <td class=\"text-center\">\r\n            <div @click=\"deleteTask(index)\">\r\n              <span class=\"fa fa-trash pointer\"></span>\r\n            </div>\r\n          </td>\r\n          <td class=\"text-center\">\r\n            <div @click=\"editTask(index)\">\r\n              <p class=\"fa fa-pen pointer\"></p>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n  </center>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"HelloWorld\",\r\n  props: {\r\n    msg: String,\r\n  },\r\n  data() {\r\n    return {\r\n      task: \"\",\r\n      editedTask: null,\r\n      statuses: [\"to-do\", \"in-progress\", \"finished\"],\r\n      /* Status could be: 'to-do' / 'in-progress' / 'finished' */\r\n      tasks: [\r\n        {\r\n          name: \"This is my first Vue\",\r\n          status: \"to-do\",\r\n        },\r\n        {\r\n          name: \"I am Very Happy\",\r\n          status: \"in-progress\",\r\n        },\r\n        {\r\n          name: \"Create YouTube video.\",\r\n          status: \"finished\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    /**\r\n     * Capitalize first character\r\n     */\r\n    capitalizeFirstChar(str) {\r\n      return str.charAt(0).toUpperCase() + str.slice(1);\r\n    },\r\n    /**\r\n     * Change status of task by index\r\n     */\r\n    changeStatus(index) {\r\n      let newIndex = this.statuses.indexOf(this.tasks[index].status);\r\n      if (++newIndex > 2) newIndex = 0;\r\n      this.tasks[index].status = this.statuses[newIndex];\r\n    },\r\n    /**\r\n     * Deletes task by index\r\n     */\r\n    deleteTask(index) {\r\n      this.tasks.splice(index, 1);\r\n    },\r\n    /**\r\n     * Edit task\r\n     */\r\n    editTask(index) {\r\n      this.task = this.tasks[index].name;\r\n      this.editedTask = index;\r\n    },\r\n    /**\r\n     * Add / Update task\r\n     */\r\n    submitTask() {\r\n      if (this.task.length === 0) return;\r\n      /* We need to update the task */\r\n      if (this.editedTask != null) {\r\n        this.tasks[this.editedTask].name = this.task;\r\n        this.editedTask = null;\r\n      } else {\r\n        /* We need to add new task */\r\n        this.tasks.push({\r\n          name: this.task,\r\n          status: \"todo\",\r\n        });\r\n      }\r\n      this.task = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.pointer {\r\n  cursor: pointer;\r\n}\r\n.noselect {\r\n  -webkit-touch-callout: none; /* iOS Safari */\r\n  -webkit-user-select: none; /* Safari */\r\n  -khtml-user-select: none; /* Konqueror HTML */\r\n  -moz-user-select: none; /* Old versions of Firefox */\r\n  -ms-user-select: none; /* Internet Explorer/Edge */\r\n  user-select: none; /* Non-prefixed version, currently\r\n                                  supported by Chrome, Edge, Opera and Firefox */\r\n}\r\n.line-through {\r\n  text-decoration: line-through;\r\n}\r\n</style>"],"sourceRoot":""}]}